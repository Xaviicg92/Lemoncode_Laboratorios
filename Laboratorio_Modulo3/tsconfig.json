{
    "compilerOptions": {
        "target": "es6",
        "module": "es6",
        "moduleResolution": "node",
        "declaration": false,
        "noImplicitAny": false,
        "allowSyntheticDefaultImports": true,
        "sourceMap": true,
        "jsx": "react",
        "noLib": false,
        "suppressExcessPropertyErrors": true,
        "skipDefaultLibCheck": true,
        "esModuleInterop": true,
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
}